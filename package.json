{
  "name": "brandi",
  "version": "1.0.0",
  "description": "Dependency injection container for TypeScript/JavaScript.",
  "exports": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/typings/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest --colors",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:typings",
    "build:esm": "esbuild src/index.ts --bundle --platform=neutral --format=esm --minify --outfile=lib/esm/index.js",
    "build:cjs": "esbuild src/index.ts --bundle --platform=neutral --format=cjs --minify --outfile=lib/cjs/index.js",
    "build:typings": "tsc -p ./tsconfig.typings.json",
    "clean": "rimraf ./lib",
    "code:lint": "prettier --check . && eslint .",
    "code:fix": "prettier --write . && eslint . --fix"
  },
  "license": "ISC",
  "author": "Vladimir Lewandowski <me@vovaspace.com> (https://vovaspace.com/)",
  "homepage": "https://github.com/vovaspace/brandi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vovaspace/brandi.git"
  },
  "bugs": {
    "url": "https://github.com/vovaspace/brandi/issues"
  },
  "keywords": [
    "di",
    "ioc",
    "dependency injection",
    "dependency inversion",
    "inversion of control",
    "container",
    "typescript",
    "javascript",
    "node"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "esbuild": "^0.8.43",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  }
}
